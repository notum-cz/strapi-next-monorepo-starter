{
  "collectionName": "components_seo_utilities_seos",
  "info": {
    "displayName": "Seo",
    "icon": "search",
    "description": ""
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "maxLength": 60,
      "required": false
    },
    "metaDescription": {
      "type": "string",
      "maxLength": 160,
      "required": false
    },
    "metaImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "keywords": {
      "type": "text",
      "regex": "[^,]+"
    },
    "twitter": {
      "type": "component",
      "component": "seo-utilities.seo-twitter",
      "repeatable": false
    },
    "og": {
      "type": "component",
      "component": "seo-utilities.seo-og",
      "repeatable": false
    },
    "applicationName": {
      "type": "string"
    },
    "canonicalUrl": {
      "type": "string"
    },
    "metaRobots": {
      "type": "enumeration",
      "default": "all",
      "enum": [
        "all",
        "index",
        "index,follow",
        "noindex",
        "noindex,follow",
        "noindex,nofollow",
        "none",
        "noarchive",
        "nosnippet",
        "max-snippet"
      ]
    },
    "structuredData": {
      "type": "json"
    }
  }
}
